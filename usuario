<?php
class Usuario {
private $IdUsuario;
private $ccUsuario;
private $nomUsuario;
private $apeUsuario;
private  $contraUsuario;

public function ingresarUsuario($user,$cc,$nombre,$apellido,$pwd){

$this->IdUsuario = $user;
$this->ccUsuario = $cc;
$this->nomUsuario = $nombre;
$this->apeUsuario = $apellido;
$this->contraUsuario = $pwd;

    }

}

?>
